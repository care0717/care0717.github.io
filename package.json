{
  "name": "care0717.github.io",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:all": "npm run build:sandbox && npm run build",
    "build:sandbox": "for dir in sandbox/*/; do if [ -f \"${dir}package.json\" ]; then project_name=$(basename \"$dir\"); cd \"$dir\"; pnpm install; pnpm build; cd ../..; if [ -d \"${dir}dist\" ]; then cp -r \"${dir}dist\" \"public/sandbox/$project_name\"; fi; fi; done",
    "dev:all": "npm run build:sandbox && vite",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router": "^7.13.0",
    "remark": "^15.0.1",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "shiki": "^3.22.0",
    "unist-util-visit": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/mdast": "4.0.4",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}
